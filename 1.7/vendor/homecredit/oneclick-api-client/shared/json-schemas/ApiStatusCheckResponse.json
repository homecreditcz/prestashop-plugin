{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "build": {
      "type": "object",
      "properties": {
        "version": {
          "type": "string",
          "description": "ATL version"
        },
        "artifact": {
          "type": "string",
          "description": "Maven artifact name"
        },
        "name": {
          "type": "string",
          "description": "Official artifact name (human readable)"
        },
        "group": {
          "type": "string",
          "description": "Artifact group name"
        },
        "time": {
          "type": "number",
          "description": "Artifact (build) creation time (timestamp)"
        }
      },
      "required": [
        "version",
        "artifact",
        "name",
        "group",
        "time"
      ]
    }
  },
  "required": [
    "build"
  ]
}